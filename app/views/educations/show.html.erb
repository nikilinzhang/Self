<!DOCTYPE html>
<html>
<head>
  <title></title>
</head>
<body>
  <div class="center">
  	<div class="center">
  	  <% @educations.each do |ed| %>
  	  <div class="title">
  	  <%= ed.Degree %>
  	  <%= ed.Range %>
  	  </div>
  	  <br>
  	  <div class="subtitle">
  	  <%= ed.School %>
  	  </div>
  	  <div class="content">
  	  <%= ed.Description %>
  	  </div>
  	  <br><br>
  	  <% end %>
    </div>
  </div>
  <div class="bottom">
    <div class="details">
    <% if @educations.first != nil%>
      <a href='/'><img src="/home.png" style="position: absolute; width:10%;height:100%left: 25%;right: 80%;"> </a>
      <a href=<%= users_path(id:@educations.first.user_id) %>><img src="/profile.png" style="position: absolute; width:10%;height:100%left: 25%;right: 60%;"> </a>
      <a href=<%= education_path(user_id:@educations.first.user_id) %>><img src="/education.png" style="position: absolute; width:10%;height:100%left: 25%;right: 40%;"> </a>
      <a href=<%= experience_path(user_id:@educations.first.user_id) %>><img src="/experience.png" style="position: absolute; width:10%;height:100%left: 75%;right: 20%;"> </a>
    <% else %>
     <a href='/'><img src="/home.png" style="position: absolute; width:10%;height:100%left: 25%;right: 75%;"> </a>
    <% end %>
    </div>
  </div>
</body>
</html>
